<h1>Warehouses</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
  <% @warehouses.each do |warehouse| %>
    <tr id="warehouse-<%= warehouse.id %>">
      <td><%= link_to "#{warehouse.name}", warehouse_path(warehouse.id) %></td>
      <td><%= link_to 'Edit', edit_warehouse_path(warehouse.id) %></td>
    </tr>
  <% end %>
</table>
<br>

<%= link_to 'Create Warehouse', new_warehouse_path %>